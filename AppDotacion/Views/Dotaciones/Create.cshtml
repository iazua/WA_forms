@model AppDotacion.Models.Dotacion

@{
    ViewData["Title"] = "Crear Dotación";
}

<h2>Crear Dotación</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Rut_DNI"></label>
        <input asp-for="Rut_DNI" class="form-control" />
        <span asp-validation-for="Rut_DNI" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Agente"></label>
        <input asp-for="Agente" class="form-control" />
        <span asp-validation-for="Agente" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Pais_Call_Center"></label>
        <select asp-for="Pais_Call_Center" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Pais_Call_Center"]))"></select>
        <input asp-for="Pais_Call_Center" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Pais_Call_Center" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Nombre_Call_Center"></label>
        <input asp-for="Nombre_Call_Center" class="form-control" />
        <span asp-validation-for="Nombre_Call_Center" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Area"></label>
        <select asp-for="Area" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Area"]))"></select>
        <input asp-for="Area" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Area" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="AreaGestion"></label>
        <select asp-for="AreaGestion" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["AreaGestion"]))"></select>
        <input asp-for="AreaGestion" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="AreaGestion" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Contrato"></label>
        <select asp-for="Contrato" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Contrato"]))"></select>
        <input asp-for="Contrato" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Contrato" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Tipos_de_agente"></label>
        <select asp-for="Tipos_de_agente" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Tipos_de_agente"]))"></select>
        <input asp-for="Tipos_de_agente" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Tipos_de_agente" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Servicio"></label>
        <select asp-for="Servicio" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Servicio"]))"></select>
        <input asp-for="Servicio" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Servicio" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Usuarios_RcWeb"></label>
        <input asp-for="Usuarios_RcWeb" class="form-control" />
        <span asp-validation-for="Usuarios_RcWeb" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Nombre_Jefatura"></label>
        <select asp-for="Nombre_Jefatura" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Nombre_Jefatura"]))"></select>
        <input asp-for="Nombre_Jefatura" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Nombre_Jefatura" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Rut_Ficticio"></label>
        <input asp-for="Rut_Ficticio" class="form-control" />
        <span asp-validation-for="Rut_Ficticio" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Rut_DNI2"></label>
        <input asp-for="Rut_DNI2" class="form-control" />
        <span asp-validation-for="Rut_DNI2" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DV"></label>
        <input asp-for="DV" type="number" class="form-control" />
        <span asp-validation-for="DV" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Clasifica_Cargo"></label>
        <select asp-for="Clasifica_Cargo" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["Clasifica_Cargo"]))"></select>
        <input asp-for="Clasifica_Cargo" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="Clasifica_Cargo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="CARGO"></label>
        <select asp-for="CARGO" class="form-control" asp-items="@(new SelectList((IEnumerable<string>)ViewData["CARGO"]))"></select>
        <input asp-for="CARGO" class="form-control mt-2" placeholder="O ingresa un valor manual" />
        <span asp-validation-for="CARGO" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Fecha_Ingreso"></label>
        <input asp-for="Fecha_Ingreso" type="date" class="form-control" />
        <span asp-validation-for="Fecha_Ingreso" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Fecha_Baja"></label>
        <input asp-for="Fecha_Baja" type="date" class="form-control" />
        <span asp-validation-for="Fecha_Baja" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="N_Personal"></label>
        <input asp-for="N_Personal" type="number" class="form-control" />
        <span asp-validation-for="N_Personal" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Correo"></label>
        <input asp-for="Correo" type="email" class="form-control" />
        <span asp-validation-for="Correo" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
